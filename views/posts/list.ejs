<!DOCTYPE html>
<html>
<head>
<title> Forum </title>
</head>
<body>
<%for (const [key, val] of Object.entries(jsonData)) {%>
    Title: <%= key%>
    <br />
    Body: <%= val%>
    <br />
    <form method="POST" action="/posts/delete/<%= key%>">
    <button type="submit"> Delete </button>
    </form>
    <br /> 
    <br />
    <% } %>
<a href="/posts/new"> create new post </a>
</body>
</html>